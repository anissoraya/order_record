<h1>Chefs</h1>
<div class="table-responsive">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th colspan="3">Manage Chef</th>
    </tr>
  </thead>

  <tbody>
    <% @chefs.each do |chef| %>
      <tr>
        <td><%= chef.name %></td>
        <td><%= chef.phone %></td>
        <td><%= link_to chef do%><i class="fa fa-eye"></i><%end%></td>
        <td><%= link_to edit_chef_path(chef) do%><i class="fa fa-pencil"></i><%end%></td>
        <td><%= link_to chef, method: :delete, data: { confirm: 'Are you sure?' }, :remote => true, :class => 'delete_chef' do%><i class="fa fa-remove"></i><%end%></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<center><%= link_to 'New Chef', new_chef_path, class: 'btn btn-info' %></center>
